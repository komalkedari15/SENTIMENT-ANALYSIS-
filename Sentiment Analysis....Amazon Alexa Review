   "cell_type": "code",
   "execution_count": 6,
   "id": "6b5fd0b8",
   "metadata": {},
   "outputs": [],
   "source": [
    "from nltk.corpus import stopwords\n",
    "stopwords = stopwords.words('english')\n",
    "\n",
    "def remove_stopword(text):\n",
    "    new_text = []\n",
    "    for i in text.split():\n",
    "        if(i in stopwords):\n",
    "            new_text.append('')\n",
    "        else:\n",
    "            new_text.append(i)\n",
    "    x = new_text[:]\n",
    "    new_text.clear()\n",
    "    return \" \".join(x) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "95e47ede",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['verified_reviews']=df['verified_reviews'].apply(remove_stopword)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "4774a694",
   "metadata": {},
   "outputs": [],
   "source": [
    "import string,time\n",
    "string.punctuation\n",
    "\n",
    "exclude=string.punctuation\n",
    "\n",
    "def remove_pun(text):\n",
    "    for i in exclude:\n",
    "        text=text.replace(i,'')\n",
    "    return text"
   ]
  },
