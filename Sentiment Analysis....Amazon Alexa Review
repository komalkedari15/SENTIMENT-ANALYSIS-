    "from keras.models import Sequential\n",
    "nn = Sequential()\n",
    "from keras.layers import Dense, Dropout\n",
    "\n",
    "nn.add(Dense(5000,input_dim=3381,activation='relu'))\n",
    "nn.add(Dense(2000,activation='relu'))\n",
    "nn.add(Dense(1000,activation='relu'))\n",
    "nn.add(Dense(1000,activation='relu'))\n",
    "nn.add(Dense(500,activation='relu'))\n",
    "nn.add(Dense(1,activation='sigmoid'))\n",
    "\n",
    "nn.compile(loss='binary_crossentropy',optimizer='Adam',metrics='accuracy')\n",
    "model = nn.fit(xtrain,ytrain,validation_data=(xtest,ytest),epochs=10)"
